#! /bin/dash

separator='âœ”\n\n'

echo '===== Cleanup brew ====='
brew cleanup
echo "$separator"

echo '===== Clean npm cache ====='
npm cache clean --force
echo "$separator"

echo '===== Clean yarn cache ====='
yarn cache clean
echo "$separator"

echo '===== Clean docker ====='
# docker-clean
docker system prune
echo "$separator"

# echo '===== Clean Xcode ====='
# xcrun simctl delete unavailable
# echo "$separator"

echo '===== Empty trash ====='
rm -rf ~/.Trash/*
echo "$separator"
